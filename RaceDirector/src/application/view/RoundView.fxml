<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.RoundController">
   <children>
      <AnchorPane prefHeight="618.0" prefWidth="660.0">
         <children>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="RED" height="43.0" opacity="0.61" stroke="BLACK" strokeType="INSIDE" width="601.0" />
            <ImageView fitHeight="702.0" fitWidth="869.0" opacity="0.38" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../data/whoop.jpg" />
               </image>
            </ImageView>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#ff1f1f" height="120.0" opacity="0.65" stroke="BLACK" strokeType="INSIDE" width="660.0" />
            <Rectangle arcHeight="5.0" arcWidth="5.0" height="43.0" stroke="#515050" strokeType="INSIDE" width="660.0" />
            <Label fx:id="roundUpdaterLabel" alignment="CENTER" layoutX="18.0" layoutY="55.0" prefHeight="58.0" prefWidth="167.0" text="Round #">
               <font>
                  <Font name="System Bold" size="40.0" />
               </font>
            </Label>
            <Button fx:id="homeButton" layoutX="240.0" layoutY="49.0" mnemonicParsing="false" onAction="#loadHome" text="Home" />
            <Button fx:id="endRaceButton" layoutX="488.0" layoutY="50.0" mnemonicParsing="false" onAction="#endRace" text="End Race" />
         </children>
      </AnchorPane>
      <Button fx:id="previousRoundButton" layoutX="78.0" layoutY="473.0" mnemonicParsing="false" onAction="#previousRoundScene" text="Previous Round" />
      <Button fx:id="startRoundButton" layoutX="267.0" layoutY="473.0" mnemonicParsing="false" onAction="#startRoundScene" text="Start Round" />
      <Button fx:id="nextRoundButton" layoutX="451.0" layoutY="473.0" mnemonicParsing="false" onAction="#nextRoundUpdateScene" text="Next Round" />
      <ScrollPane layoutX="88.0" layoutY="120.0" prefHeight="267.0" prefViewportHeight="265.0" prefViewportWidth="490.0" prefWidth="440.0">
         <content>
            <Label fx:id="heatDisplayLabel" alignment="TOP_LEFT" layoutX="88.0" layoutY="120.0" prefHeight="660.0" prefWidth="505.0" />
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
